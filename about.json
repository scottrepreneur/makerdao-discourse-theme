  "color_schemes": {
    "Maker Light": {
      "primary": "53546a",                  // Body Gray  | Most text, icons, and borders.
      "secondary": "FFFFFF",                // White      | The main background color, and text color of some buttons.
      "tertiary": "3BAB94",                 // Light Green| Links, some buttons, notifications, and accent color.
      "quaternary": "1AAB9B",               // Maker Teal | Navigation links.
      "header_background": "1AAB9B",        // Maker Teal | Background color of the site's header.
      "header_primary": "53546a",           // Body Gray  | Text and icons in the site's header.
      "highlight": "1AAB9B",                // Maker Teal | The background color of highlighted elements on the page, such as posts and topics.
      "danger": "96000E",                   // Red        | Highlight color for actions like deleting posts and topics.
      "success": "1CA551",                  // Green      | Used to indicate an action was successful.
      "love": "F50057"                      // Lighter Red| The like button's color.
    },
    "Maker Dark": {
      "primary": "F8FAFC",
      "secondary": "333333",
      "tertiary": "E4F6F2",
      "quaternary": "179b8c",
      "header_background": "179b8c",
      "header_primary": "333333",
      "highlight": "00DDC3",
      "danger": "96000e",
      "success": "1ca551",
      "love": "f50057"
    }
  }
}

